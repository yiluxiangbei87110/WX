.headerText image{
  height:60rpx;
  width: 60rpx;
  border-radius: 50%;
}
.wrap{
    flex-direction:column;
    display:flex;
    margin-top:20rpx;
    margin-bottom:40rpx;
    margin-left: 0rpx;
    background-color:#fff;
    border-bottom: 1px solid #ededed;
    border-top: 1px solid #ededed;
    padding-bottom: 5px;
}
.wrap-header-text{
      margin-top:10rpx;
      margin-bottom: 20rpx;
      margin-left: 10px;
}
.wrap-header{
    width:60rpx;
    height:60rpx;
    vertical-align: middle;
}
.wrap-text{
    margin-left: 20px;
    vertical-align: middle;
}

.wrap-image{
    width:100%;
    height:340rpx;
    margin:auto 0;
    margin-bottom: 15px;
}


.wrap-title{
    font-size:34rpx;
    font-weight: 600;
    color:#333;
    margin-bottom: 10px;
    margin-left: 10px;
}
.wrap-content{
    color:#666;
    font-size:28rpx;
    margin-bottom:20rpx;
    margin-left: 20rpx;
    letter-spacing:2rpx;
    line-height: 40rpx;
}
.wrap-like{
    font-size:13px;
    line-height: 16px;
    margin-left: 10px;
}

.wrap-like-image{
    height:16px;
    width:16px;
    margin-right: 8px;
    vertical-align:middle;
}

.wrap-like-font{
    vertical-align:middle;
    margin-right: 20px;
}
