<import src="../movie-single-tpl/movie-single.wxml" />
<template name="more-movie">
<scroll-view class="grid-container" scroll-y="true" scroll-x="false"  bindscrolltolower="onScrollLower">
    <block wx:for="{{movies}}" wx:for-item="movie">
        <view class="single-view-container">
          <template is="movie-single" data="{{...movie}}"></template>
        </view>
    </block>
</scroll-view>
</template>